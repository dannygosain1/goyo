      forest;
    end fork
    :5 fold cross
    validation;
  fork again
    :30% validation
    subset;
  end fork
  :test each model against
  30% validation subset;
  :report;
  fork
    :accuracy;
  fork again
    :precsion;
  fork again
    :recall;
  fork again
    :F1 score;
  end fork
  end
